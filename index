          <script>
            document.addEventListener('DOMContentLoaded', function() {
                var checkbox = document.getElementById('changeTheme');
                checkbox.addEventListener('change', function() {
                    var theme = this.checked ? 'dark' : 'light'; // Toggle theme based on checkbox
                    document.documentElement.setAttribute('data-theme', theme); // Apply theme to the document
                    localStorage.setItem('theme', theme); // Save theme preference
                });
            
                // Initialize theme from localStorage or system preference
                !function () {
                    try {
                        var d = document.documentElement, n = 'data-theme', s = 'setAttribute';
                        var e = localStorage.getItem('theme');
                        if ('system' === e || (!e && false)) {
                            var t = '(prefers-color-scheme: dark)', m = window.matchMedia(t);
                            if (m.media !== t || m.matches) {
                                d.style.colorScheme = 'dark'; d[s](n, 'dark');
                            } else {
                                d.style.colorScheme = 'light'; d[s](n, 'light');
                            }
                        } else if (e) {
                            d[s](n, e || '');
                            document.getElementById('changeTheme').checked = (e === 'dark'); // Set checkbox based on saved theme
                        } else {
                            d[s](n, 'matrica');
                        }
                        if (e === 'light' || e === 'dark') d.style.colorScheme = e;
                    } catch (e) {}
                }();
            });
            </script>-->

